  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "bleach": "rm -rf node_modules/ && rm -rf .next/ && npm install",
    "type-check": "tsc --skipLibCheck --noEmit",
    "lint": "next lint",
    "format": "prettier . --write",
    "check": "npm run type-check && npm run format && npm run lint",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "updatePackages:major": "npx taze major --write && npm install",
    "updatePackages:minor": "npx taze minor --write && npm install",
    "prepare": "husky"
  },
  "dependencies": {
    "@headlessui/react": "^2.1.2",
    "@heroicons/react": "^2.1.5",
    "clsx": "^2.1.1",
