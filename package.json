    "nuqs": "^1.17.4",
    "prettier-plugin-organize-imports": "^3.2.4",
    "zod": "^3.23.8",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "typescript": "^5",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "eslint": "^8",
    "eslint-config-next": "14.2.5",
    "@types/eslint": "^8.56.10",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react-compiler": "^0.0.0-experimental-51a85ea-20240601",
