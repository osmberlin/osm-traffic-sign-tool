{
	"name": "my-app",
	"version": "0.0.12",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"type-check": "tsc --noEmit",
		"lint": "eslint .",
		"format": "prettier . --write . && prettier **/*.svelte --write .",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json && npm run type-check && npm run lint && npm run format && npm run test",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test": "npm run test:unit:run && npm run test:playwright",
		"test:playwright": "echo \"playwright test skipped\" # playwright test",
		"test:unit": "vitest",
		"test:unit:run": "vitest run",
		"test:unit:ui": "vitest --ui",
		"test:unit:coverage": "vitest run --coverage",
		"data:wiki": "npm run data:wiki:downloadWiki && npm run data:wiki:parseWiki && npm run data:wiki:downloadSvgs && npm run data:wiki:applyParsedWiki",
		"data:wiki:downloadWiki": "vite-node ./src/data/wiki/downloadWiki/downloadWiki.js && prettier ./src/data/ --write",
		"data:wiki:parseWiki": "vite-node ./src/data/wiki/parseWiki/parseWiki.ts && prettier ./src/data/ --write",
		"data:wiki:downloadSvgs": "vite-node ./src/data/wiki/downloadSvgs/downloadSvgs.ts",
		"data:wiki:applyParsedWiki": "vite-node ./src/data/wiki/parseWiki/applyParsedWiki.ts && prettier ./src/data/ --write",
		"deploy": "chmod +x ./deploy.sh && ./deploy.sh",
		"updatePackages": "npx taze major --write",
		"updatePackages:minor": "npx taze minor --write",
		"prepare": "husky install"
	},
	"devDependencies": {
		"@playwright/test": "^1.42.1",
		"@sveltejs/adapter-auto": "^2.1.1",
		"@sveltejs/adapter-static": "^2.0.3",
		"@sveltejs/kit": "^1.30.4",
		"@typescript-eslint/eslint-plugin": "^6.21.0",
		"@typescript-eslint/parser": "^6.21.0",
		"autoprefixer": "^10.4.19",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.35.1",
		"husky": "^8.0.3",
		"postcss": "^8.4.38",
		"prettier": "^3.2.5",
		"prettier-plugin-svelte": "^3.2.2",
		"svelte": "^4.2.12",
		"svelte-check": "^3.6.8",
		"sveltekit-search-params": "^1.1.1",
		"tailwindcss": "^3.4.3",
		"tslib": "^2.6.2",
		"typescript": "^5.4.3",
		"vite": "^4.5.3",
		"vitest": "^0.34.6"
	},
	"type": "module",
	"dependencies": {
		"@fontsource/overpass": "^5.0.17",
		"@fontsource/overpass-mono": "^5.0.17",
		"@tabler/icons-svelte": "^2.47.0",
		"@tailwindcss/forms": "^0.5.7",
		"@tailwindcss/typography": "^0.5.12",
		"@total-typescript/ts-reset": "^0.5.1",
		"axios": "^1.6.8",
		"cheerio": "^1.0.0-rc.12",
		"prettier-plugin-tailwindcss": "^0.5.13",
		"tailwind-merge": "^1.14.0",
		"vite-node": "^0.34.7"
	}
}
